<h1>Изменение профиля</h1>
<div class="row">
  <%= render partial: 'shared/edit_tabs', locals: {user_id: @user.id} %>
</div>

<div class="row">
  <div class="col-md-4 col-md-offset-3">
    <%= form_for(@restaurant_type) do |f| %>
        <%= render partial: 'restaurant_types/partials/restaurant_types_form_fields',
                   locals: {f: f} %>
        <%= f.submit 'Сохранить', class: 'btn btn-lg btn-primary' %>
    <% end %>

  </div>
</div>
  