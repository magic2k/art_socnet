<h1>Альбом: редактирование</h1>

<!-- navbar with tabs-->
<div class="row">
  <%= render partial: 'shared/edit_tabs', locals: {user_id: @user.id} %>
</div>

<div class="container">
  <div class="row">
    "Добавить фотографии"
    <%= form_for @album, html: {multipart: true} do |f| %>
      <%= f.label 'add images' %>
      <%= f.file_field :images %>
        <%= f.submit 'добавить', class: 'btn btn-lg btn-primary' %>
    <% end  %>
  </div>
</div>
